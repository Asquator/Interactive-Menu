<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuController">
   <center>
      <VBox fx:id="entries" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#orderClicked" text="Order" BorderPane.alignment="CENTER">
         <font>
            <Font name="Comfortaa Regular" size="18.0" />
         </font></Button>
   </bottom>
   <top>
      <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Our Menu" BorderPane.alignment="CENTER">
         <font>
            <Font name="Comfortaa Bold" size="24.0" />
         </font></Label>
   </top>
</BorderPane>
